{
  "name": "devkit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/devkit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/devkit",
        "main": "packages/devkit/src/index.ts",
        "tsConfig": "packages/devkit/tsconfig.lib.json",
        "assets": [
          "packages/devkit/*.md",
          {
            "input": "./packages/devkit/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/devkit/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/devkit",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/devkit",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/devkit/**/*.ts",
          "packages/devkit/generators.json",
          "packages/devkit/executors.json",
          "packages/devkit/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/devkit/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
