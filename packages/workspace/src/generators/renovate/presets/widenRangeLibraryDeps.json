{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchDepTypes": ["dependencies", "peerDependencies", "optionalDependencies", "devDependencies"],
      "matchUpdateTypes": ["minor", "patch", "bump"],
      "excludePaths": ["package.json"],
      "enabled": false
    }
  ]
}
