{
  "$schema": "http://json-schema.org/schema",
  "name": "workspace",
  "version": "0.0.1",
  "generators": {
    "codecov": {
      "factory": "./src/generators/codecov/generator.compat#codecovSchematic",
      "schema": "./src/generators/codecov/schema.json",
      "description": "Configures Codecov"
    },
    "contributors": {
      "factory": "./src/generators/contributors/generator.compat#contributorsSchematic",
      "schema": "./src/generators/contributors/schema.json",
      "description": "Adds contributors section to Readme.md"
    },
    "eslint": {
      "factory": "./src/generators/eslint/generator.compat#eslintSchematic",
      "schema": "./src/generators/eslint/schema.json",
      "description": "Configures ESLint with additional rules"
    },
    "github-workflow": {
      "factory": "./src/generators/github-workflow/generator.compat#gitHubWorkflowSchematic",
      "schema": "./src/generators/github-workflow/schema.json",
      "description": "Configures GitHub CI Workflow"
    },
    "prettier": {
      "factory": "./src/generators/prettier/generator.compat#prettierSchematic",
      "schema": "./src/generators/prettier/schema.json",
      "description": "Configure Prettier"
    },
    "renovate": {
      "factory": "./src/generators/renovate/generator.compat#renovateSchematic",
      "schema": "./src/generators/renovate/schema.json",
      "description": "Configures Renovate"
    },
    "tsconfig": {
      "factory": "./src/generators/tsconfig/generator.compat#tsConfigSchematic",
      "schema": "./src/generators/tsconfig/schema.json",
      "description": "Configures tsconfig.base.json"
    }
  }
}
