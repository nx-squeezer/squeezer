{
  "name": "ngx-forms",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ngx-forms/src",
  "prefix": "nx-squeezer",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/ngx-forms/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/ngx-forms/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/ngx-forms/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest"
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "postTargets": ["ngx-forms:github", "ngx-forms:npm"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github"
    },
    "npm": {
      "executor": "ngx-deploy-npm:deploy"
    }
  },
  "tags": []
}
