{
  "name": "ngx-async-injector",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ngx-async-injector/src",
  "prefix": "nx-squeezer",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/ngx-async-injector/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/ngx-async-injector/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/ngx-async-injector/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest"
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "postTargets": ["ngx-async-injector:github", "ngx-async-injector:npm"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github"
    },
    "npm": {
      "executor": "ngx-deploy-npm:deploy"
    },
    "docs": {
      "executor": "@twittwer/compodoc:compodoc",
      "options": {
        "coverageMinimumPerFile": 0
      }
    }
  },
  "tags": []
}
