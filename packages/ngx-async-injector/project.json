{
  "name": "ngx-async-injector",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ngx-async-injector/src",
  "prefix": "nx-squeezer",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/ngx-async-injector/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/ngx-async-injector/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/ngx-async-injector/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/ngx-async-injector/jest.config.ts",
        "codeCoverage": true
      },
      "configurations": {
        "watch": {
          "watch": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ngx-async-injector/**/*.ts", "packages/ngx-async-injector/**/*.html"]
      }
    }
  },
  "tags": []
}
