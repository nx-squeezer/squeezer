{
  "name": "nx-squeezer",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Monorepo for Nx and Angular tooling and libraries",
  "keywords": [
    "angular",
    "nx"
  ],
  "author": "samuelfernandez",
  "homepage": "https://github.com/nx-squeezer/squeezer#readme",
  "bugs": {
    "url": "https://github.com/nx-squeezer/squeezer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nx-squeezer/squeezer.git"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.5.5"
  },
  "private": true,
  "scripts": {
    "lint": "nx run-many --target=lint --parallel=3 --all",
    "lint:fix": "nx run-many --target=lint --parallel=3 --all --fix",
    "nx": "nx",
    "lint:workspace": "nx workspace-lint",
    "g": "cross-env NODE_OPTIONS=\"-r ts-node/register\" nx g",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@jscutlery/semver": "^2.29.2",
    "@nrwl/devkit": "15.2.4",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "node-fetch-commonjs": "3.2.4",
    "prettier": "2.8.0",
    "renovate": "34.54.0",
    "yaml": "2.1.3"
  },
  "devDependencies": {
    "@commitlint/config-angular": "^17.3.0",
    "@delagen/eslint-plugin-deprecation": "1.3.2",
    "@nrwl/cli": "15.2.4",
    "@nrwl/eslint-plugin-nx": "15.2.4",
    "@nrwl/jest": "15.2.4",
    "@nrwl/js": "15.2.4",
    "@nrwl/linter": "15.2.4",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/nx-plugin": "15.2.4",
    "@nrwl/workspace": "15.2.4",
    "@schemastore/eslintrc": "0.0.6",
    "@schemastore/package": "0.0.6",
    "@schemastore/prettierrc": "0.0.9",
    "@schemastore/tsconfig": "0.0.9",
    "@swc-node/register": "1.5.4",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.22",
    "@swc/helpers": "0.4.14",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.11.12",
    "@types/which": "2.0.1",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.16.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jsonc-eslint-parser": "2.1.0",
    "nx": "15.2.4",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.8.4"
  },
  "workspaces": [
    "packages/**"
  ]
}
